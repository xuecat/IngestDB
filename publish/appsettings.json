{
  "Logging": {
    "Level": 4,
    "SaveDays": 15,
    "MaxFileSize": "10MB",
    "Path": "/opt/ingestdbsvr/nmlog",
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Port": 9024,
  "UseSwagger": 1,
  "AllowedHosts": "*",
  "PollySetting": {
    "TimeoutTime": 10, //请求超时多少秒触发其他机制
    "RetryCount": 3, //单个请求异常重试次数
    "CircuitBreakerOpenFallCount": 2, //断路器异常次数触发
    "CircuitBreakerDownTime": 6, //断路器自启等待时间秒
    "CircuitBreakerOpenTriggerCount": 3 //断路器触发次数执行操作
  },
  "Limit24Hours": false,
  "NotifyUdpInfomation": true
}
